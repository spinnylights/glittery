<h2>Add New Artwork</h2>
<%= form_for @artwork, url: { action: 'create' } do |a| %>
  <%= a.label      :image %>
  <%= a.file_field :image %><br>

  <%= a.label      :name %>
  <%= a.text_field :name %><br>

  <%= a.label     :description %><br>
  <%= a.text_area :description %><br>

  <%= a.submit 'Submit' %>
<% end %>
<%= link_to 'Back', '/' %>
